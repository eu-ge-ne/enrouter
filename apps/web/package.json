{
  "name": "@enrouter/web",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    "./ssr": "./dist/server/ssr.js",
    "./manifest": "./dist/client/.vite/manifest.json"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "vite",
    "lint": "tsc",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --config configs/vite.config.ts",
    "build:server": "vite build --config configs/vite.config.ssr.ts",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@enrouter/tsconfig": "workspace:*",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@vitejs/plugin-react": "^4.3.2",
    "enrouter": "workspace:*",
    "globals": "^15.10.0",
    "react": "19.0.0-rc-1460d67c-20241003",
    "react-dom": "19.0.0-rc-1460d67c-20241003",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
