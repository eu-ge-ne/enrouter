{
  "name": "enrouter",
  "version": "0.0.3",
  "type": "module",
  "description": "",
  "exports": {
    ".": {
      "types": "./dist/lib/mod.d.ts",
      "default": "./dist/lib/mod.js"
    },
    "./vite/plugin": {
      "types": "./dist/vite/plugin/mod.d.ts",
      "default": "./dist/vite/plugin/mod.js"
    },
    "./vite/manifest": {
      "types": "./dist/vite/manifest.d.ts",
      "default": "./dist/vite/manifest.js"
    }
  },
  "imports": {
    "#lib/*": "./lib/src/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./coverage",
    "lint": "tsc -p lib/tsconfig.json && tsc -p vite/tsconfig.json",
    "test": "vitest run --config configs/vitest.config.ts",
    "build": "true",
    "build:lib": "tsup --config configs/tsup.lib.config.ts",
    "build:vite": "tsup --config configs/tsup.vite.config.ts"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:*"
        ]
      },
      "build:lib": {
        "cache": true,
        "outputs": [
          "{projectRoot}/dist/lib"
        ],
        "dependsOn": [
          "^build",
          "clean"
        ]
      },
      "build:vite": {
        "cache": true,
        "outputs": [
          "{projectRoot}/dist/vite"
        ],
        "dependsOn": [
          "^build",
          "clean"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eu-ge-ne/enrouter.git"
  },
  "keywords": [
    "React",
    "Router"
  ],
  "author": "Eugene Pobochny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eu-ge-ne/enrouter/issues"
  },
  "homepage": "https://github.com/eu-ge-ne/enrouter#readme",
  "peerDependencies": {
    "vite": "^5.4.8"
  },
  "devDependencies": {
    "@enrouter/tsconfig": "workspace:*",
    "@types/lodash.groupby": "^4.6.9",
    "@types/react": "npm:types-react@rc",
    "@vitest/browser": "^2.1.3",
    "@vitest/coverage-v8": "^2.1.3",
    "glob": "^11.0.0",
    "lodash.groupby": "^4.6.0",
    "playwright": "^1.48.0",
    "react": "19.0.0-rc-1460d67c-20241003",
    "regexparam": "^3.0.0",
    "terser": "^5.34.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "vitest-browser-react": "^0.0.1"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc"
  }
}
