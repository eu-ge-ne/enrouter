{
  "name": "enrouter",
  "version": "0.0.10",
  "type": "module",
  "description": "",
  "exports": {
    ".": {
      "types": "./dist/lib/mod.d.ts",
      "default": "./dist/lib/mod.js"
    },
    "./vite/plugin": {
      "types": "./dist/vite/plugin/mod.d.ts",
      "default": "./dist/vite/plugin/mod.js"
    },
    "./vite/manifest": {
      "types": "./dist/vite/manifest/mod.d.ts",
      "default": "./dist/vite/manifest/mod.js"
    }
  },
  "imports": {
    "#lib/*": "./lib/src/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./coverage",
    "build:lib": "tsup --config configs/tsup.config.lib.ts",
    "build:vite:manifest": "tsup --config configs/tsup.config.vite.manifest.ts",
    "build:vite:plugin": "tsup --config configs/tsup.config.vite.plugin.ts",
    "lint:lib": "tsc -p lib/tsconfig.json",
    "lint:vite:manifest": "tsc -p vite/manifest/tsconfig.json",
    "lint:vite:plugin": "tsc -p vite/plugin/tsconfig.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules vitest run --config configs/vitest.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eu-ge-ne/enrouter.git"
  },
  "keywords": [
    "React",
    "Router"
  ],
  "author": "Eugene Pobochny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eu-ge-ne/enrouter/issues"
  },
  "homepage": "https://github.com/eu-ge-ne/enrouter#readme",
  "devDependencies": {
    "@enrouter/tsconfig": "workspace:*",
    "@types/lodash.groupby": "^4.6.9",
    "@types/react": "npm:types-react@rc",
    "@vitest/browser": "^2.1.3",
    "@vitest/coverage-v8": "^2.1.3",
    "glob": "^11.0.0",
    "lodash.groupby": "^4.6.0",
    "playwright": "^1.48.2",
    "react": "19.0.0-rc-1460d67c-20241003",
    "regexparam": "^3.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.3",
    "vitest-browser-react": "^0.0.3"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc"
  }
}
